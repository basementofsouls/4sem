    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="roll.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DotImaging.BitmapSourceInterop" Version="4.8.3" />
